<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="LONGVARBINARY" hibernate-type="binary"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="7"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="19" not-null="false">
		</sql-type>
		<sql-type jdbc-type="REAL" hibernate-type="float" precision="12"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="128" not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-name="Jobs" match-catalog="JobDB"></table-filter>
	<table-filter match-name="JobJDLs" match-catalog="JobDB"></table-filter>
	<table-filter match-name="JobParameters" match-catalog="JobDB"></table-filter>
	<table catalog="JobDB" name="Jobs"></table>
	<table catalog="JobDB" name="JobJDLs"></table>
	<table catalog="JobDB" name="JobParameters"></table>
</hibernate-reverse-engineering>