<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="64" not-null="true">
		</sql-type>
		<sql-type jdbc-type="LONGVARBINARY" hibernate-type="binary"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="19" not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-name="ProxyDB_Proxies"
		match-catalog="ProxyDB">
	</table-filter>
	<table catalog="ProxyDB" name="ProxyDB_Proxies">
		<column name="UserDN"></column>
		<column name="UserGroup"></column>
		<column name="UserName"></column>
		<column name="Pem"></column>
		<column name="PersistentFlag"></column>
		<column name="ExpirationTime"></column>
	</table>
</hibernate-reverse-engineering>